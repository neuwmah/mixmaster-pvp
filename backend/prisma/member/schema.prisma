generator client {
  provider = "prisma-client-js"
  output   = "../generated/mysql/member"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_MEMBER_URL")
}

model ChangePassword {
  AdminID  String   @default("") @db.VarChar(12)
  id_idx   Int?
  PlayerID String   @default("") @db.VarChar(12)
  Passwd   String   @default("") @db.VarChar(30)
  RegDate  DateTime @default(dbgenerated("'0001-01-01 01:01:01'")) @db.DateTime(0)

  @@id([PlayerID, AdminID])
}

model Distraint {
  id         Int       @id @default(autoincrement())
  player_id  String?   @db.VarChar(255)
  old_ep     Int?
  new_ep     Int?
  updated_by String?   @db.VarChar(255)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
}

model Distraint2 {
  Num          Int                  @id @default(autoincrement()) @db.UnsignedInt
  id_idx       Int                  @default(0) @db.UnsignedInt
  PlayerID     String               @default("") @db.VarChar(12)
  CPPlayerID   String               @default("") @db.VarChar(20)
  CPCode       Int                  @default(0) @db.UnsignedTinyInt
  AdminID      String               @default("") @db.VarChar(20)
  BlockType    Distraint2_BlockType @default(GAME)
  Access       Distraint2_Access    @default(ALLOW)
  CharName     String?              @db.VarChar(20)
  ServerName   String?              @db.VarChar(50)
  Subject      String               @default("") @db.VarChar(255)
  Content      String               @db.Text
  StartDate    DateTime             @default(dbgenerated("'0001-01-01 01:01:01'")) @db.DateTime(0)
  EndDate      DateTime             @default(dbgenerated("'0001-01-01 01:01:01'")) @db.DateTime(0)
  RegDate      DateTime             @default(dbgenerated("'0001-01-01 01:01:01'")) @db.DateTime(0)
  B_Flag       Distraint2_B_Flag    @default(Y)
  BlockTypeNum Int?                 @default(0)

  @@index([PlayerID], map: "PlayerID")
  @@index([id_idx], map: "id_idx")
}

model Management {
  Num           Int             @id @default(autoincrement()) @db.UnsignedInt
  AdminID       String          @default("") @db.VarChar(20)
  Passwd        String          @default("") @db.VarChar(30)
  Name          String          @default("") @db.VarChar(30)
  Team          String          @default("") @db.VarChar(30)
  Jigham        String          @default("") @db.VarChar(30)
  Email         String          @default("") @db.VarChar(30)
  ZipCode       String          @default("") @db.VarChar(7)
  Address       String          @default("") @db.VarChar(255)
  Tphone        String          @default("") @db.VarChar(20)
  Cphone        String          @default("") @db.VarChar(20)
  StartDate     DateTime        @default(dbgenerated("'0001-01-01'")) @db.Date
  EndDate       DateTime        @default(dbgenerated("'0001-01-01'")) @db.Date
  Access        Int             @default(10) @db.TinyInt
  Flag          Management_Flag @default(Y)
  nCount        Int             @default(0) @db.UnsignedInt
  LoginIP       String?         @db.VarChar(15)
  LastLoginDate DateTime?       @db.DateTime(0)
  RegDate       DateTime        @default(dbgenerated("'0001-01-01 01:01:01'")) @db.DateTime(0)
}

model Mark {
  id_idx   Int?
  PlayerID String? @db.VarChar(255)

  @@ignore
}

model Player {
  id_idx          Int                @default(autoincrement()) @db.UnsignedInt
  PlayerID        String             @unique(map: "PlayerID") @default("") @db.VarChar(255)
  Passwd          String             @default("") @db.VarChar(255)
  EP              BigInt             @default(0)
  Passwd_A        String?            @db.VarChar(64)
  Passwd_Q        String?            @db.Char(2)
  Name            String             @default("") @db.VarChar(20)
  JuminNo         String             @default("") @db.VarChar(20)
  nYear           String             @default("") @db.VarChar(4)
  nMonth          String             @default("") @db.Char(2)
  nDay            String?            @db.Char(2)
  Sex             String             @default(dbgenerated("1"))
  TelePhone1      String?            @db.VarChar(20)
  CPhone1         String?            @db.VarChar(13)
  CPhone2         String             @default("") @db.VarChar(4)
  CPhone3         String             @default("") @db.VarChar(4)
  ZipCode         String             @default("") @db.VarChar(10)
  AddressDo       String?            @db.VarChar(20)
  AddressSi       String?            @db.VarChar(30)
  AddressDong     String?            @db.VarChar(50)
  AddressEtc      String?            @db.VarChar(100)
  Address         String?            @db.VarChar(255)
  Email           String?            @db.VarChar(255)
  Access          Int                @default(21) @db.UnsignedTinyInt
  Block           Player_Block?      @default(ALLOW)
  LoginIP         String?            @db.VarChar(15)
  NewsLetter      String?            @default("1") @db.Char(1)
  ParentName      String?            @db.VarChar(20)
  ParentJuminNo   String?            @db.VarChar(20)
  ParentPhone1    String?            @db.VarChar(13)
  ParentPhone2    String             @default("") @db.VarChar(4)
  ParentPhone3    String             @default("") @db.VarChar(4)
  LastLoginDate   DateTime?          @db.DateTime(0)
  SecederDate     DateTime?          @db.DateTime(0)
  PayPlayDate     DateTime?          @db.DateTime(0)
  PayPlayHours    Int?               @default(0) @db.UnsignedInt
  RegDate         DateTime           @default(dbgenerated("'0001-01-01 00:00:00'")) @db.DateTime(0)
  OldBlock        Player_OldBlock    @default(ALLOW)
  ssoChk          String?            @db.Char(1)
  AuthCheck       Player_AuthCheck   @default(AUTH_OK)
  AuthTimeLimit   DateTime           @default(dbgenerated("'0001-01-01 00:00:00'")) @db.DateTime(0)
  LoginState      Boolean            @default(false)
  personal_auth   String?            @default("N") @db.Char(1)
  Accesschecktype Int?               @default(0)
  avatar          String             @default("0") @db.VarChar(255)
  IP              String?            @db.VarChar(255)
  token           String?            @db.VarChar(255)
  isValidateToken Int                @default(0)
  PlayTime        Int?               @default(0)
  OnPoints        String?            @default("1") @db.VarChar(255)
  TelePhone2      String?            @db.VarChar(4)
  TelePhone3      String?            @db.VarChar(4)
  JobType         String?            @db.Char(2)
  affiliate_id    Int?
  GPBank          String             @default("0") @db.VarChar(20)
  gems            BigInt             @default(0)
  online_points   Int                @default(0)

  @@id([id_idx, RegDate])
  @@index([Name], map: "Name")
  @@index([RegDate], map: "RegDate")
  @@index([JuminNo, Block, id_idx], map: "g_JuminNo")
  @@index([PlayerID, JuminNo, Block, RegDate], map: "p_index1")
}

