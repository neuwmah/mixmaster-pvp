  status   Int    @default(0)
  type     Int
  method   Int
  itemid   Int
  quantity Int
  date     String @db.VarChar(32)
}

model code_promo {
  code      String? @db.VarChar(255)
  pts_bonus String? @db.VarChar(255)
  id_use    String? @db.VarChar(255)

  @@ignore
}

model general_config {
  id         Int       @id @default(autoincrement())
