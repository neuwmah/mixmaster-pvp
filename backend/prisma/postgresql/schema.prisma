generator client {
  provider = "prisma-client-js"
  output   = "../generated/postgresql"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Changelog {
  id         String   @id @default(cuid())
  created_at DateTime @default(now())
  slug       String   @unique
  title      String
  image_src  String?
  content1   String?
  content2   String?
  active     Boolean? @default(false)
}

model Menu {
  id         String   @id @default(cuid())
  created_at DateTime @default(now())
  title      String
  url        String
}
